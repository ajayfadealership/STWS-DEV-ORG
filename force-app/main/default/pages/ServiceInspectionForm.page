        <script>
        var objMap = new Object();
        $(document).ready( 
            function() {
                console.log("Ready()");
                if("{!flag}" == "1") {
                    console.log("{!flag}");
                    console.log("{!objDF.Id}");
                    objMap["Id"] = "{!objDF.Id}";
                    BOATBUILDING.ServiceInspectionFormController.getformRecord("{!objDF.Id}", function(result, event) {
                        if(result != null) { 
                            console.log(result);
                            var keys = Object.keys(result);
                            console.log(keys);
                            for(var i = 0; i < keys.length; i++) {
                                if(document.getElementById(keys[i]+":"+result[keys[i]]) != null 
                                   && (result[keys[i]].indexOf('Need Now') != -1 || result[keys[i]].indexOf('Need Soon') != -1 || result[keys[i]].indexOf('OK') != -1 
                                   || result[keys[i]].indexOf('Accept') != -1 || result[keys[i]].indexOf('Do not Fix'))) {
                                    console.log("Key: "+keys[i]);
                                    console.log("Val: "+result[keys[i]]);
                                    document.getElementById(keys[i]+":"+result[keys[i]]).click();
                                } 
                            }
                        }
                    });	
                }
            } 
        );
        
        function getValueForMap(obj) {
            //console.log(obj);
            console.log(obj.value);
            var val = obj.value;
            var spltVal = val.split(":");
            objMap[spltVal[0]] = spltVal[1];
            console.log(spltVal[0]);
            console.log(objMap[spltVal[0]]);
            
            document.getElementById("lab"+spltVal[0]+":Need Now").style.background = "white";
            document.getElementById("lab"+spltVal[0]+":Need Soon").style.background = "white";
            document.getElementById("lab"+spltVal[0]+":OK").style.background = "white";
            
