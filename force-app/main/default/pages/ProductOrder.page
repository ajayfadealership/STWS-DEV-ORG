          			}
          			
          			$("#closeIcn").click(function() {
          				$("#doId").fadeOut();
          			});
          			
          			function addDealerOptionToList(obj) {
          				console.log(obj);
          				var dec = '';
          				var retPrice = 0.00;
          				var dealPrice = 0.00;
          				var prtNo = Math.floor(Math.random()*100000);
          				
          				if($("#decId").val() != null && $("#decId").val() != '') {
          					dec = $("#decId").val();
          				}
          				if($("#retId").val() != null && $("#retId").val() != '') {
          					retPrice = $("#retId").val();
          				}
          				if($("#delId").val() != null && $("#delId").val() != '') {
          					dealPrice = $("#delId").val();
          				}
          				var taxcheck = '0';
          				if(document.getElementById("taxcheckDO").checked) {
          					taxcheck = '1';
          				}
          				
          				if(dec != '' && retPrice != '' && dealPrice != '') {
          					addDealOptToLst(dec, dealPrice, retPrice, prtNo, taxcheck);
          				} else {
          					alert("Please fill all the details.")
          				}
          			}
          			function completedTaskListUpdate() {
          				$("#decId").val("");
          				$("#retId").val("");
          				$("#delId").val("");
          				document.getElementById("taxcheckDO").checked = false;
          				$("#doId").fadeOut();
          			} 
          			function isNumberKey(evt) {
			            var charCode = (evt.which) ? evt.which : evt.keyCode;
			            if (charCode != 45 && charCode != 46 && charCode > 31 
			                && (charCode < 48 || charCode > 57)){
			                return false;  
			            }
			            return true;
			        }
