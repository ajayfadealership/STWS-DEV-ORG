    <script> 
    function saveDetails() {
        var email = document.getElementById("pg:frm:eml").value;
        if(document.getElementById("pg:frm:SSN").value == null || document.getElementById("pg:frm:SSN").value == "" ) {
            alert("Please Enter Social Security Number.");
            return false;
        }
        
        if(document.getElementById("pg:frm:sadd").value == null || document.getElementById("pg:frm:sadd").value == "" ) {
            alert("Please Enter street address.");
            return false;
        }
        
        if(document.getElementById("pg:frm:city").value == null || document.getElementById("pg:frm:city").value == "" ) {
            alert("Please Enter City.");
            return false;
        }
        
        if(document.getElementById("pg:frm:oc").value == null || document.getElementById("pg:frm:oc").value == "" ) {
            alert("Please Enter Occupation.");
            return false;
        }
        
        if(document.getElementById("pg:frm:zip").value == null || document.getElementById("pg:frm:zip").value == "" ) {
            alert("Please Enter Zip/Postal Code.");
            return false;
        }
        
        if(document.getElementById("pg:frm:slryag").value == null || document.getElementById("pg:frm:slryag").value == "" ) {
            alert("Please Enter Salary (Annually Gross).");
            return false;
        }
        
        if(document.getElementById("pg:frm:taemp").value == null || document.getElementById("pg:frm:taemp").value == "" ) {
            alert("Please Enter Years at employer.");
            return false;
        }
        
        if(email != "") {
            saveDetailsSF();
        } 	
        else {
            alert("Please enter email.");
        }
    }
    </script>
