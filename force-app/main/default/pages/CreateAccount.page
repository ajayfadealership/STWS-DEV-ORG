                    mapAccountFieldValues["ItemStatus"] = "NO";
                }
            }
            $('.popup').addClass('active');
            BOATBUILDING.CreateAccountController.SaveAccount(mapAccountFieldValues, function(result, event) {
                console.log(JSON.stringify(event));
                console.log('result',JSON.stringify(result));
                if(event.status) {
                    
                    if(result != null) {
                        if(invParamAssign != '' && invParamAssign == 'PartInvoiceTab') {
