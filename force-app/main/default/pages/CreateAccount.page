                //Boat Make
                if(document.getElementById("pge:frm:btMake_lkid").value != ""
                   && document.getElementById("pge:frm:btMake_lkid").value != null
                   && document.getElementById("pge:frm:btMake_lkid").value != "000000000000000") {
                    mapAccountFieldValues["BoatMake"] = document.getElementById("pge:frm:btMake_lkid").value; 
                }
                else {
                    //mapAccountFieldValues["BoatMake"] = " ";
                    document.getElementById("errSnap").innerHTML = "Please Select Boat Make.";
                    $("#errorDiv").slideDown();
                    $("#errorDiv").delay(8000).slideUp();
                    return;
                }
                
                //Boat Model
                if($("#btMdl").val().trim() != "") {
                    mapAccountFieldValues["BoatModel"] = $("#btMdl").val(); 
                }
                else {
                    //mapAccountFieldValues["BoatModel"] = " ";
                    document.getElementById("errSnap").innerHTML = "Please Enter Boat Model.";
                    $("#errorDiv").slideDown();
                    $("#errorDiv").delay(8000).slideUp();
                    return;
                }
                
                //Boat Year
                if($("#btYear").val().trim() != "") {
                    mapAccountFieldValues["BoatYear"] = $("#btYear").val(); 
                }
                else {
                    //mapAccountFieldValues["BoatYear"] = " "; 
                    document.getElementById("errSnap").innerHTML = "Please Enter Boat Year.";
                    $("#errorDiv").slideDown();
                    $("#errorDiv").delay(8000).slideUp();
                    return;
                }
                
                //Boat Registration Number
                if($("#btRegNumber").val().trim() != "") {
                    mapAccountFieldValues["BoatRegistrationNumber"] = $("#btRegNumber").val(); 
                }
                
                
            }
            else {
