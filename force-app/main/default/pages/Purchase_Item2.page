                border-bottom: 1px solid #d8dde6;
            }
            .Customer-Heading{
                border-top: 1px solid #e0e5ee;
                padding-top: 1rem;
                margin: 1rem -0.75rem;
            }
            .slds-input{
                padding: 0px 0px 0px 12px !important;
            }
            .lookupIcon{
                display:none;
            }
        </style>
        
    </head>
    <body>
        <script>
        		$(document).ready(function() {
        			console.log("{!$CurrentPage.parameters.conId}");
        			console.log("{!$CurrentPage.parameters.conName}");
        			var conId = "{!$CurrentPage.parameters.conId}";
        			var conName = "{!$CurrentPage.parameters.conName}";
        			if(conId != "" && conId != undefined 
        				&& conName != "" && conName != undefined) {
        				document.getElementById("existingCID").checked = true;
	        				document.getElementById('errorSpanId').style.display = "none";
	                if(document.getElementById("existingCID").checked == true){
	                    document.getElementById('Customer_Datails').style.display = "none";
	                    document.getElementById('nameLookUpId').style.display = "block";
	                }else if(document.getElementById("existingCID").checked == false){
	                    document.getElementById('Customer_Datails').style.display = "block";
	                    document.getElementById('nameLookUpId').style.display = "none";
	                }
	                document.getElementById("p:frm:lookupInputId_lkid").value = conId;
	                document.getElementById("p:frm:lookupInputId").value = conName;
	                document.getElementById("p:frm:lookupInputId_lkold").value = conName;
        			} else {
        				document.getElementById('stockInvtryId').checked = false;
	                document.getElementById('existingCID').checked = false;
	                document.getElementById('Customer_Datails').style.display = "block";
	                document.getElementById('nameLookUpId').style.display = "none";
	                document.getElementById('opfromsDivId').style.display = "block";
	                document.getElementById('opforsDivId').style.display = "none";
        			}
        		});
            function existingCheck(obj){
                //alert('Checked Status: '+document.getElementById(obj.id).checked);
                document.getElementById('errorSpanId').style.display = "none";
                if(document.getElementById(obj.id).checked == true){
                    document.getElementById('Customer_Datails').style.display = "none";
                    document.getElementById('nameLookUpId').style.display = "block";
                }else if(document.getElementById(obj.id).checked == false){
                    document.getElementById('Customer_Datails').style.display = "block";
                    document.getElementById('nameLookUpId').style.display = "none";
                }
            }
            function orderForDealerCheck(obj){
//              alert(obj.id);
                document.getElementById('errorSpanId').style.display = "none";
                if(document.getElementById(obj.id).checked == true) {
                    document.getElementById('Customer_Datails').style.display = "none";
                    document.getElementById('nameLookUpId').style.display = "none";
                    document.getElementById('existCheckDivId').style.display = "none";
                    document.getElementById('existingCID').checked = false;
                    document.getElementById('opforsDivId').style.display = "block";
                    document.getElementById('opfromsDivId').style.display = "none";
                    
                    document.getElementById('p:frm:lookupInputId').value = '';
                    document.getElementById('inputEmail').value = '';
                    document.getElementById('inputName').value = '';
                    
                }else if(document.getElementById(obj.id).checked == false){
                    document.getElementById('Customer_Datails').style.display = "block";
                    document.getElementById('nameLookUpId').style.display = "none";
                    document.getElementById('existCheckDivId').style.display = "block";
                    document.getElementById('existingCID').checked = false;
                    document.getElementById('opforsDivId').style.display = "none";
                    document.getElementById('opfromsDivId').style.display = "block";
                }
            }
            function hideError(){
                var strManName = document.getElementById('p:frm:sl1').options[document.getElementById('p:frm:sl1').selectedIndex].text;
                
                if(strManName.indexOf('Moomba') != -1){
                    document.getElementById('errorSpanId').style.display = "none";
                }else if(strManName.indexOf('Supra') != -1 || strManName.indexOf('Malibu') != -1){
                    if(document.getElementById('p:frm:sl4').options[document.getElementById('p:frm:sl4').selectedIndex].text != null
                        && document.getElementById('p:frm:sl4').options[document.getElementById('p:frm:sl4').selectedIndex].text != ''
                            && document.getElementById('p:frm:sl4').options[document.getElementById('p:frm:sl4').selectedIndex].text != '--None--'){
                            document.getElementById('errorSpanId').style.display = "none";
                        }
                }
            }
            onload = function(){
                /*document.getElementById('stockInvtryId').checked = false;
                document.getElementById('existingCID').checked = false;
                document.getElementById('Customer_Datails').style.display = "block";
                document.getElementById('nameLookUpId').style.display = "none";
                document.getElementById('opfromsDivId').style.display = "block";
                document.getElementById('opforsDivId').style.display = "none";*/
            }
            
        </script>
        <apex:form id="frm" >
            <a class="slds-button slds-button--neutral slds-button--brand" style="border: 1px solid white; box-shadow: 3px 5px 15px #888888; position: fixed; z-index: 99; left: 1%; top: 2%;" href="/home/home.jsp">Home</a> 
            <apex:messages style="color : red;"></apex:messages>
                
                <apex:inputHidden value="{!objAccount.Name}" id="onjAccNameId"/>
