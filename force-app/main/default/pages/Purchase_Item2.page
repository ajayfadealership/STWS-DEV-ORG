                <apex:inputHidden value="{!objAccount.Desire_boat_model__c}" id="onjAccDBModId"/>
                <apex:inputHidden value="{!objAccount.Prospect_Desired_Boat_Length__c}" id="onjAccPDBLId"/>
                <apex:inputHidden value="{!objAccount.Desire_Price_low__c}" id="onjAccDPLId"/>
                <apex:inputHidden value="{!objAccount.Desire_price_high__c}" id="onjAccDPHId"/>
                
                <apex:inputHidden value="{!buttonRenderForPlaceOrder}" id="buttonRenderForPlaceOrderId"/>
                <apex:inputHidden value="{!buttonRenderForSaveAndReCal}" id="buttonRenderForSaveAndReCalId"/>
                
                <apex:actionFunction name="callCancelPage" action="{!cancelPage}" id="af1"/>
