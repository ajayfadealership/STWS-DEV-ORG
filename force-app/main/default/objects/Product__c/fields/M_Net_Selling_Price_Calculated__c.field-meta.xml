<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>M_Net_Selling_Price_Calculated__c</fullName>
    <deprecated>false</deprecated>
    <description>Calculated by Subtracting Trade Allowances from Your Price</description>
    <externalId>false</externalId>
    <formula>IF((M_Boat_Trade_Allowance__c +  M_Trailer_Trade_Allowance__c)&lt; M_Your_Price_Calculated__c  ,M_Your_Price_Calculated__c - (M_Boat_Trade_Allowance__c +  M_Trailer_Trade_Allowance__c)  ,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Net Selling Price (Calculated)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
