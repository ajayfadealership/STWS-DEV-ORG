<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cant_reopen_Work_Order</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED( Status__c) &amp;&amp; ISPICKVAL(PRIORVALUE(Status__c),&apos;Closed&apos;)</errorConditionFormula>
